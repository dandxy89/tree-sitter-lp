{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "lp",
      "scope": "source.lp",
      "path": ".",
      "file-types": [
        "lp"
      ],
      "highlights": "queries/highlights.scm",
      "locals": "queries/locals.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for LP (Linear Programming) files",
    "authors": [
      {
        "name": "Dan"
      }
    ],
    "links": {
      "repository": "https://github.com/dandxy89/tree-sitter-lp"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}